{
  "name": "nyTaxiAnalytics",
  "virtualClusterId": "d3tgzq8tnp8i5gndcnn3euwe1",
  "executionRoleArn": "arn:aws:iam::668876353122:role/spark",
  "releaseLabel": "emr-6.2.0-latest",
  "jobDriver": {
    "sparkSubmitJobDriver": {
      "entryPoint": "s3://gromav-test/spark-eks-assembly-2.0.jar",
      "entryPointArguments": ["s3://nyc-tlc/trip\\ data/yellow_tripdata_2017-*.csv,s3://nyc-tlc/trip\\ data/yellow_tripdata_2018-*.csv,s3://nyc-tlc/trip\\ data/yellow_tripdata_2019-*.csv,s3://nyc-tlc/trip\\ data/yellow_tripdata_2020-*.csv", "s3://nyc-tlc/trip\\ data/green_tripdata_2017-*.csv,s3://nyc-tlc/trip\\ data/green_tripdata_2018-*.csv,s3://nyc-tlc/trip\\ data/green_tripdata_2019-*.csv,s3://nyc-tlc/trip\\ data/green_tripdata_2020-*.csv","s3://nyc-tlc/misc/taxi\\ _zone_lookup.csv","s3://gromav-test/results"],
      "sparkSubmitParameters": "--class ValueZones"
    }
  },
  "configurationOverrides": {
    "applicationConfiguration": [
      {
        "classification": "spark-defaults",
        "properties": {
          "spark.dynamicAllocation.executorAllocationRatio": "1",
          "spark.dynamicAllocation.shuffleTracking.enabled": "true",
          "spark.dynamicAllocation.shuffleTracking.timeout": "120s",
          "spark.dynamicAllocation.enabled":"true",
          "spark.executorEnv.AWS_REGION": "us-east-1",
          "spark.kubernetes.driverEnv.AWS_REGION": "us-east-1",
          "spark.executor.memory": "27G",
          "spark.driver.memory": "8G",
          "spark.executor.cores": "3",
          "spark.driver.node.selector": "lifecycle:od",
          "spark.executor.node.selector": "lifecycle:spot"
        }
      }
    ],
    "monitoringConfiguration": {
      "persistentAppUI": "ENABLED",
      "cloudWatchMonitoringConfiguration": {
        "logGroupName": "/emr-containers/jobs",
        "logStreamNamePrefix": "demo"
      },
      "s3MonitoringConfiguration": {
        "logUri": "s3://gromav-test/emr-eks"
      }
    }
  }
}